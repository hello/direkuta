[Unit]
Description=Stream event handler for EvoStream Server
After=network.target

[Service]
Type=simple
ExecStart=/usr/bin/java -jar /home/ubuntu/direkuta-0.0.1-SNAPSHOT.jar server /home/ubuntu/direkuta.dev.yml
TimeoutStopSec=0
Restart=always

[Install]
WantedBy=default.target
Alias=direkuta.service